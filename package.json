{
	"name": "prometheus",
	"type": "module",
	"description": "Starter project for Simple",
	"license": "CC-BY-NC-3.0",
	"repository": {
		"type": "git",
		"url": "https://bitbucket.org/SimpleIM/prometheus"
	},
	"scripts": {
		"dev": "concurrently 'node --watch-path=./src/icons/ svg.js' 'npx vite'",
		"build": "node svg.js && npx vite build"
	},
	"engines": {
		"node": ">=18.11.0",
		"npm": ">=8.19.2"
	},
	"dependencies": {
		"@alpinejs/collapse": "^3.12.2",
		"@alpinejs/focus": "^3.12.3",
		"@alpinejs/intersect": "^3.12.2",
		"@alpinejs/mask": "^3.12.3",
		"@alpinejs/persist": "^3.12.3",
		"alpinejs": "^3.12.2",
		"rollup-plugin-copy": "^3.4.0"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.3",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"glob": "^10.2.7",
		"postcss": "^8.4.24",
		"postcss-import": "^15.1.0",
		"postcss-nesting": "^11.3.0",
		"postcss-preset-env": "^8.5.0",
		"svgo": "^3.0.2",
		"svgstore": "^3.0.0-2",
		"tailwindcss": "^3.3.2",
		"vite": "^4.3.9",
		"vite-plugin-image-optimizer": "^1.1.6",
		"vite-plugin-restart": "^0.3.1"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"Firefox ESR",
		"not ie 10",
		"not op_mini all",
		"not op_mob 12.1",
		"not baidu 7.12",
		"not ie_mob 10",
		"not ie_mob 11",
		"not and_uc 11.8"
	],
	"eslintConfig": {
		"parser": "babel-eslint",
		"plugins": [
			"compat"
		],
		"rules": {
			"compat/compat": "warn"
		}
	},
	"prettier": {
		"trailingComma": "es5",
		"useTabs": true,
		"tabWidth": 4,
		"semi": false,
		"singleQuote": false
	}
}
